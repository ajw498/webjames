
# Makefile for WebJames the web server

CC            = cc

MemCheck = MemCheck.cc

# possible flags:
# ANTRESOLVER     use the ANT resolver module API for reverse DNS
# LOG             enable logging
# SYSLOG          use SysLog instead of internal log
#Flags         = -DLOG
Flags         = -DANTRESOLVER -DLOG
#Flags         = -DANTRESOLVER -DLOG -DSYSLOG

#CCFlags       = -c -throwback -depend !Depend -IC: -strict -fah -Otime $(Flags)
CCFlags       = -c -throwback -depend !Depend -IC: -g -strict -fah $(Flags) -Wg
LinkFlags     = -o $@ -d

Libs          = C:OSLib.o.OSLib C:o.Stubs C:o.socklib MemCheck:o.MC C:regex.a.libregex
Objects       = main.o webjames.o ip.o stat.o report.o write.o openclose.o read.o cache.o coding.o mimemap.o attributes.o resolve.o cacheheap.o handler.o staticcontent.o webjamesscript.o cgiscript.o sendasis.o

!RunImage:    $(Objects)
              drLink $(Linkflags) $(Objects) $(Libs)
#              Squeeze -f -v !RunImage

.c.o:
	$(MemCheck) $(CC) $(CCFlags) $< -o $@

# Dynamic dependencies:
o.coding: c.coding h.coding
o.mimemap: c.mimemap h.mimemap C:h.kernel
o.cacheheap: c.cacheheap C:h.stdio C:h.swis C:h.kernel h.cacheheap
o.ip: c.ip C:h.stdio C:h.string C:h.stdlib C:oslib.h.socket C:oslib.h.types C:oslib.h.os h.ip
o.stat: c.stat C:h.time C:h.stdio C:h.stdlib C:h.string C:oslib.h.os C:oslib.h.types h.webjames h.stat h.ip
o.openclose: c.openclose C:h.stdio C:h.stdlib C:h.string C:h.ctype C:h.time h.cache h.webjames h.ip h.attributes C:regex.h.regex h.stat h.openclose h.resolve
o.cache: c.cache MemCheck:h.MemCheck C:h.stddef C:h.stdio C:h.stdarg C:h.stdio C:h.stdlib C:h.string C:h.time C:oslib.h.os C:oslib.h.types C:oslib.h.wimp C:oslib.h.osspriteop C:oslib.h.font C:oslib.h.fileswitch C:oslib.h.osfile C:oslib.h.osfscontrol h.webjames h.cache h.write h.cacheheap h.stat h.mimemap
o.attributes: c.attributes C:h.stdio C:h.stdlib C:h.string C:h.ctype C:h.time h.webjames h.write h.attributes C:regex.h.regex h.stat h.handler C:oslib.h.osfscontrol C:oslib.h.types C:oslib.h.os C:oslib.h.fileswitch
o.resolve: c.resolve MemCheck:h.MemCheck C:h.stddef C:h.stdio C:h.stdarg C:h.string C:h.stdio C:h.time h.stat h.webjames h.resolve h.ip C:h.swis C:h.kernel
o.main: c.main MemCheck:h.MemCheck C:h.stddef C:h.stdio C:h.stdarg C:h.time C:h.ctype C:h.stdio C:h.string C:h.stdlib C:oslib.h.os C:oslib.h.types C:oslib.h.wimp C:oslib.h.osspriteop C:oslib.h.font h.main h.webjames h.global
