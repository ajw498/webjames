# Makefile for WebJames the web server
#
# Settings for the debug build

#Programs
CC = cc
LINK = link
MEMCHECK =
SQUEEZE = Settype $@ ff8
SETTYPE = SetType $@ Obey

#Libraries
MEMCHECKLIB =
STUBS = C:o.stubs
OSLIB = OSLib:o.OSLib
REGEX = C:regex.a.libregex

#Compiler flags
THROWBACK = -throwback
DEPEND = -depend !Depend
WARNINGS = -strict -fah
OPTIMISATION =
DEBUG = -g -Wg
INCLUDE = -IC: -IOSLib:

#Linker flags
LINKDEBUG = -d

#!Run file
WIMPSLOT = $(WIMPSLOT_DEBUG)
STDERR =
TYPESTDERR =
