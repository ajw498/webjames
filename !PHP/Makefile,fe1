

LIBS	=	\
		C:o.stubs\
		C:tboxlibs.o.eventlib\
		C:tboxlibs.o.toolboxlib\
		C:tboxlibs.o.wimplib


OBJECTS = Main.o

INCLUDE = C:

TARGET = !RunImage

CCFLAGS = $(CCEXTRA) -throwback -depend !Depend -strict -fah -Otime -I$(INCLUDE)


CC = cc -c $(CCFLAGS)

all: $(TARGET) PHPMail

PHPMail: c.PHPMail
	gcc -mthrowback -O2 -IOSLib: -o PHPMail c.PHPMail -lOSLib:o.OSLib

.c.o:
	$(CC) $< -o $@




$(TARGET):	$(OBJECTS)
	link -o $@ $(OBJECTS) $(LIBS)


# Dynamic dependencies:
o.Main: c.Main C:tboxlibs.h.wimp C:tboxlibs.h.toolbox C:h.kernel C:tboxlibs.h.event C:tboxlibs.h.wimplib C:tboxlibs.h.gadgets
