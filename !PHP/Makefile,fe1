HEADERS	=	

LIBS	=	\
		L:AJWLib.o.AJWLib\
		L:stubs.o.asstubs\
		L:Desk.o.Desk


OBJECTS	=	!RunImage.o

INCLUDE	=	L:

TARGET		=	!RunImage

CCFLAGS		=	$(CCEXTRA) -mthrowback -Wall -O3 -I$(INCLUDE)


CC		=	gcc -c $(CCFLAGS)


.c.o:
	$(CC) $< -o $@




$(TARGET):	$(OBJECTS) $(LIBS)
	gcc -o $@ $(OBJECTS) $(LIBS)

