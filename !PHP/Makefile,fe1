

LIBS	=	\
		C:o.stubs\
		C:tboxlibs.o.eventlib\
		C:tboxlibs.o.toolboxlib\
		C:tboxlibs.o.wimplib


OBJECTS = Main.o

INCLUDE = C:

TARGET = !RunImage

CCFLAGS = $(CCEXTRA) -throwback -depend !Depend -strict -fah -Otime -I$(INCLUDE)


CC = cc -c $(CCFLAGS)


.c.o:
	$(CC) $< -o $@




$(TARGET):	$(OBJECTS)
	link -o $@ $(OBJECTS) $(LIBS)


# Dynamic dependencies:
o.Main:	c.Main
o.Main:	C:tboxlibs.h.wimp
o.Main:	C:tboxlibs.h.toolbox
o.Main:	C:h.kernel
o.Main:	C:tboxlibs.h.event
o.Main:	C:tboxlibs.h.wimplib
o.Main:	C:tboxlibs.h.gadgets
