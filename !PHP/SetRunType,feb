
Set Alias$@RunType_18A WimpSlot -Min 1300k -Max 1300k |M  Run <PHP$Dir>.NoCGI %%*0

If "<PHP$TaskWindow>"="Yes" Then Set Alias$@RunType_18A TaskWindow "Run <PHP$Dir>.NoCGI %%*0" -name "PHP script" -quit -wimpslot 1300k

If "<PHP$CGI>"="Yes" Then Set Alias$@RunType_18A WimpSlot -Min 1300k -Max 1300k |M Run <PHP$Dir>.CGIWrapper %%*0

