# Makefile for WebJames the web server
#
# $Id: normal,v 1.3 2002/10/05 16:59:17 ajw Exp $
#
# Settings for the normal build

#Programs
CC = cc
GCC = gcc
LINK = link
LN_S = @echo ln -s
MEMCHECK =
SQUEEZE = Squeeze -f $@
SETTYPE = SetType $@ Obey

#Libraries
MEMCHECKLIB =
STUBS = C:o.stubs
OSLIB = OSLib:o.OSLib
OSLIBSUPPORT = C:OSLibSupport.o.OSLibSupport
REGEX = C:regex.libregex

#Compiler flags
THROWBACK = -throwback
DEPEND = -depend !Depend
WARNINGS = -strict -fah
OPTIMISATION = -Otime
DEBUG =
INCLUDE = -IC: -IOSLib: -IC:regex
GCCINCLUDE = -IL: -IOSLib:

#Linker flags
LINKDEBUG =

#!Run file
WIMPSLOT = $(WIMPSLOT_SCL)
STDERR = .*
TYPESTDERR = .*
