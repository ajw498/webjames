# Makefile for WebJames the web server
#
# $Id: cross,v 1.8 2002/10/05 16:18:47 ajw Exp $
#
# Settings for the gcc/unixlib build using the gccsdk cross compiler

#Programs
CC = gcc
GCC = gcc
LINK = gcc
MEMCHECK =
SQUEEZE =
SETTYPE =
LN_S = ln -s

#Libraries
MEMCHECKLIB =
STUBS =
OSLIB = -loslib
OSLIBSUPPORT = -loslibsupport
REGEX = -lregex

#Compiler flags
THROWBACK =
DEPEND =
WARNINGS = -Wall -Wundef -Wpointer-arith -Wmissing-prototypes
OPTIMISATION = -O2
DEBUG = 
INCLUDE = -I$(LIBDIR)
GCCINCLUDE = $(INCLUDE)

#Linker flags
LINKDEBUG = -L$(LIBDIR)/lib

#File Extensions
EXT_FEB = ,feb
EXT_FF8 = ,ff8
EXT_FAF = ,faf

#!Run file
WIMPSLOT = $(WIMPSLOT_UNIXLIB)
STDERR = .*
TYPESTDERR = .*

all2: all

#Legacy target
setup:

