# Makefile for WebJames the web server
#
# $Id: debug,v 1.3 2002/10/05 16:59:17 ajw Exp $
#
# Settings for the debug build

#Programs
CC = cc
GCC = gcc
LINK = link
LN_S = @echo ln -s
MEMCHECK =
SQUEEZE = Settype $@ ff8
SETTYPE = SetType $@ Obey

#Libraries
MEMCHECKLIB =
STUBS = C:o.stubs
OSLIB = OSLib:o.OSLib
OSLIBSUPPORT = C:OSLibSupport.o.OSLibSupport
REGEX = C:regex.libregex

#Compiler flags
THROWBACK = -throwback
DEPEND = -depend !Depend
WARNINGS = -strict -fah
OPTIMISATION =
DEBUG = -g
INCLUDE = -IC: -IOSLib: -IC:regex
GCCINCLUDE = -IL: -IOSLib:

#Linker flags
LINKDEBUG = -d

#!Run file
WIMPSLOT = $(WIMPSLOT_DEBUG)
STDERR =
TYPESTDERR =
